/* 
##############################
###	        FOOTER         ###
##############################
*/
#footerback {
	background:url("../img/footer.png") no-repeat left top;
	height: 45px;
	z-index:-1;
}

.orangecontainer {
	
	margin-top:45px;
	height: 175px;
	width: 199px;
	float:left;
	z-index:0;
	position:relative;
	background-color: #F6E3CE;
	-webkit-box-shadow: 0px 8px 6px 0px rgba(220, 220, 220, 0.45);
-moz-box-shadow:    0px 8px 6px 0px rgba(220, 220, 220, 0.45);
box-shadow:         0px 8px 6px 0px rgba(220, 220, 220, 0.45);

border: 1px solid #F6E3CE;
border-right: 1px solid #f09a1e;
border-top: 1px solid #f09a1e;
border-left: 0px solid #fff;
}
.orangecontainer li{
	margin-top:-45px;
}
.orangecontainer article{
	margin-top:-45px;
	display:block;
	width: 195px;
}
.oheader {
	
	font-size: 10pt;
	height:20px;
	padding-top:50px;
	font-weight: bold;
	text-align:center;
	z-index:99w;
	margin: 10px 0 10px;
}
.takeawayheader {
	height: 70px;
	font-size: 10pt;
	padding-top:50px;
	font-weight: bold;
	text-align:center;
	margin: 10px 0 10px;
}
#footerback li {
	display:inline-block;
	text-align:center;
	height: 45px;
	line-height: 45px;
	vertical-align: middle;
	min-width:200px;
	width: 16,6%;
	float:left;
	font-family: 'Open Sans Condensed', sans-serif;
	color: #fff;
	font-size: 1.4em;

}
#footerback p {
	font-family: 'Open Sans', sans-serif;
	text-align:center;
	color: #000;
	font-size: 9pt;
	margin: 2px 2px 2px 2px;
}
#footerback footer {
	font-family: 'Open Sans', sans-serif;
	font-size: 7pt;
	color: #fff;
	text-align:right;
	position: absolute;
	bottom:0px;
	right: 0px;
	padding: 0 8px;
	background-color: #f09a1e;
	border-radius: 6px 0 0 0;
}
#footerback footer p{
	
}
#footerback article {
	height:220px;
}