/* main.css */
/* 
##############################
###	       GLOBALS         ###
##############################
*/
@import url(http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,300italic,700|Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,700,800,600);
@import url(http://fonts.googleapis.com/css?family=Raleway:700);
html, html a, p, a:visited, a:hover, li {
    text-rendering: optimizeLegibility !important;
	-webkit-font-smoothing: subpixel-antialiased !important;
    text-shadow: 0 0 1px rgba(0,0,0,0.3);
}
a {
	color: #fff;
	text-decoration: none;
	transition: color 0.15s linear;
}
a:visited {
	color: #fff;
}
ul {
	list-style-type: none;
    margin: 0;
    padding: 0;
	display: inline;
}
li {
    display: inline;
}
#footer {
	margin-top: 15px;
	margin-left: 82px;
	/*background-color: #f09a1e;   Old browsers */
	width:1200px;
	height:38px;
}
.clearfloat {
	clear: both;
}
/* 
##############################
###	        HEADER         ###
##############################
*/
#headerback {
	background:url("../img/header.png") no-repeat left top;
	height: 90px;
	margin: 0 0 10px 0;
	-webkit-box-shadow: 0px 8px 6px 0px rgba(220, 220, 220, 0.45);
-moz-box-shadow:    0px 8px 6px 0px rgba(220, 220, 220, 0.45);
box-shadow:         0px 8px 6px 0px rgba(220, 220, 220, 0.45);
}
#headerback a img{
	position: absolute;
	top: 5px;
	left: 2px;
	display: inline-block;
	margin: 0 10px 0 0;
	width:78px;
}
.navL1{
	position: relative;
	top: 20px;
	left: 180px;
	display: inline;
	margin: 0 10px;	
}
#headerback nav{
	margin: 20px 10px 0 0;
	width: 90%;
	display:inline;
}
.navL1 li{
	display:inline-block;
	font-family: 'Open Sans Condensed', sans-serif;
	width: 215px;
	min-width:190px;
	vertical-align: middle;
	height: 70px;
	line-height: 70px;
	text-align:center;
	font-size: 2em;
	color: #fff;
}
.navL2 {
	height: 45px;
}
.navL2 li {
	display:block;
	font-family: 'Open Sans Condensed', sans-serif;
	width: inherit;
	vertical-align: middle;
	height: 35px;
	line-height: 35px;
	text-align:left;
	font-size: 1.2em;
	color: #fff;
	margin-left:10px;
	margin-right:-10px;
	background-color: #233477;
}
.navL1 li:hover > a {
	color: #fafafa;
}

.navL1 li:hover > ul,
.navL1 li:hover > li{
	opacity: 1;
    height: auto;
	/*display:block;*/
	
}

.navL1 ul {
	display: block;
    -webkit-transition: opacity 0.5s ease-out;
    opacity: 0; 
    height: 0;
    overflow: hidden;
  
	/*display:none;*/
	position:relative;
	top:0px;
	left:0px;
	z-index:9999;
	margin: 0;
	padding: 0;
	background-color: #233477;
	font-size: 15px;
}

#page {
	min-width: 1368px;
	margin-left:auto;
	margin:0 auto 0
}

#page nav {
	height: 90px;
	margin: 0 0 10px 0;
	width: 1282px;
}
/* 
##############################
###	     FRONTPAGE         ###
##############################
*/
